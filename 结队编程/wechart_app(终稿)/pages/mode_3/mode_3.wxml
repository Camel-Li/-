<!--pages/mode_3/mode_3.wxml-->
<bgm />
<view class="content">
    <!--pics文件夹下的background.jpg文件-->
    <image class='background' src="https://s2.loli.net/2022/10/12/SI3nfH1FmZoUqOM.gif" mode="aspectFill"></image>
    <!--页面其它部分-->
</view>
<view hover-class="button_hover_list" hover-start-time="20" hover-stay-time="70" bindtap='onBackTap'>
    <image class='back' src='https://s1.ax1x.com/2022/10/12/xUu9p9.png'></image>
</view>
<view class="cover"> </view>
<view class='main'>

    <view class="title">
        <image src="https://s1.ax1x.com/2022/10/12/xUYxzT.png"></image>
    </view>
    <view>
        <text class="t1">{{text1}}</text>
    </view>
    <view class="rule">
        <image src="{{tp}}"></image>
    </view>
    <view>
        <view hover-class="button_hover_list" hover-start-time="20" hover-stay-time="70"><text class="t2" bindtap="jmp1">{{text3}}\n</text></view>
        <text>\n</text>
        <view hover-class="button_hover_list" hover-start-time="20" hover-stay-time="70"><text class="t2" bindtap="jmp2">{{text2}}</text></view>
    </view>
</view>
<view class='author'>
    <text>小组：这是个队名</text>
</view>