<!--pages/win/win.wxml-->
<bgm />
<view class="content">
    <!--pics文件夹下的background.jpg文件-->

    <image class='background' src="https://s2.loli.net/2022/10/12/SI3nfH1FmZoUqOM.gif" mode="aspectFill"></image>

    <!--页面其它部分-->
</view>
<view class='cover'></view>
<view class='main'>
    <text class='t1'>棋手{{player}}获胜</text>
    <view>
        <text class='t2'>棋手一得分：{{bot}}\n</text>
        <text class='t2'>棋手二得分：{{person}}</text>
    </view>

    <view class='ig'>
        <view hover-class="button_hover_list" hover-start-time="20" hover-stay-time="70" bindtap="jmp1">
            <image class='i1' src='https://s1.ax1x.com/2022/10/12/xUMJzj.png'></image>
        </view>
        <view hover-class="button_hover_list" hover-start-time="20" hover-stay-time="70" bindtap="jmp2">
            <image class='i2' src='https://s1.ax1x.com/2022/10/12/xUMQdf.png'></image>
        </view>
    </view>
</view>